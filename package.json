{
  "name": "mlingual",
  "version": "2.0.0",
  "description": "An opinionated build tool for making apps multilingual",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/semistandard --verbose | ./node_modules/.bin/snazzy && ./node_modules/.bin/tape 'test/**/*.js' | tap-summary",
    "test:dev": "./node_modules/.bin/tape 'test/**/*.js' | tap-summary",
    "example:flat": "node ./examples/example-flat",
    "example:not-flat": "node ./examples/example-not-flat"
  },
  "author": {
    "name": "Alexey Gorokhov",
    "email": "alexey.gorokhov@icloud.com",
    "url": ""
  },
  "license": "MIT",
  "devDependencies": {
    "proxyquire": "^1.7.10",
    "semistandard": "^11.0.0",
    "sinon": "^2.3.4",
    "snazzy": "^7.0.0",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "del": "^3.0.0",
    "globby": "^6.0.0",
    "mkdirp": "^0.5.1",
    "through2": "^2.0.1"
  },
  "keywords": [
    "language",
    "internationalization",
    "i18n",
    "multilingual",
    "dictionary",
    "template"
  ],
  "homepage": "https://github.com/AlexeyGorokhov/mlingual",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexeyGorokhov/mlingual"
  },
  "bugs": {
    "url": "https://github.com/AlexeyGorokhov/mlingual/issues"
  }
}
